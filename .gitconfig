[user]
    name = Brian Lam
    email = brianlam@stanford.edu
[core]
	editor = vim
    autocrlf = input
	excludesfile = /Users/brianlam/.gitignore
[merge]
    tool = vimdiff
[push]
	default = simple
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    lg = log -p
[color]
    diff = auto
[credential]
	helper = osxkeychain
